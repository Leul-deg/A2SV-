# Enter your code here. Read input from STDIN. Print output to STDOUT
n, m = list(map(int,input().split()))
nums = list(map(int,input().split()))
a = set(list(map(int,input().split())))
b = set(list(map(int,input().split())))
score = 0
for num in nums:
    if num in a:
        score += 1
    elif num in b:
        score -= 1
print(score)
